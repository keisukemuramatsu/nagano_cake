<div class="container">
      <div class="row">
        <%= form_with model:@newgenre, url:'admin/genres', local:true do |f| %>
          <div class="form-group">
            <%= f.label :name,"ジャンル名", class: "control-label" %>
            <%= f.text_field :name, class: "form-control book_title" %>
          </div>
          <div class="actions">
            <%= f.submit "新規登録", class: "btn btn-success" %>
          </div>
        <% end %>            
      </div>
    </div>